<?xml version="1.0"?>
<component name="application.database.manager">
	<implementation class="org.eclipse.osee.framework.database.internal.ApplicationDatabaseManager"/>
	<service>
		<provide interface="org.eclipse.osee.framework.database.IApplicationDatabaseManager"/>
	</service>
	<reference name="application.database.provider"
		interface="org.eclipse.osee.framework.database.core.IDatabaseInfoProvider"
		bind="addDatabaseProvider"
		unbind="removeDatabaseProvider"
		cardinality="0..n"
		policy="dynamic"/>
</component>